{
  "version": 2,
  "builds": [
    {
      "src": "dashboard/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/simulation-data.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/cv-vehicle-data.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/auth.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/simulation-data",
      "dest": "/api/simulation-data.py"
    },
    {
      "src": "/api/cv-vehicle-data",
      "dest": "/api/cv-vehicle-data.py"
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/logo(.*).png",
      "dest": "/logo$1.png"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "build": {
    "env": {
      "REACT_APP_API_URL": "https://smart-traffic-management-system-khaki.vercel.app/api",
      "REACT_APP_SOCKET_URL": "https://smart-traffic-management-system-khaki.vercel.app",
      "PUBLIC_URL": "",
      "CI": "false"
    }
  }
}